{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "es2020",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "types": ["node"]
  },
  "include": [
    "docusaurus.config.ts",
    "sidebars.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}


